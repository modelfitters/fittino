<!DOCTYPE InputFile PUBLIC "" "InputFile.dtd">

<InputFile>

  <VerbosityLevel
   Level = "ALWAYS"
   />

  <ExecutionMode
   Mode = "SAMPLING"
   />

  <Model
   ModelType  = "Rosenbrock"
   Dimensions = "2"
   X1         = "2.56"
   X2         = "-1.54"
   />

  <Sampler
   SamplerType = "Simple"
   />

</InputFile>
