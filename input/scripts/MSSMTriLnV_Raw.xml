<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="style/InputFile.xsl"?>

<InputFile>

    <VerbosityLevel>INFO</VerbosityLevel>

    <Model>

        <ModelBase>

            <Name>MSSM</Name>

            <ModelParameter>
                <Name>M1</Name>
                <Value>4000.</Value>
                <Error>0.1</Error>
                <LowerBound>-5000.</LowerBound>
                <UpperBound>5000.</UpperBound>
                <Fixed>true</Fixed>
            </ModelParameter>

            <ModelParameter>
                <Name>M2</Name>
                <Value>4000.</Value>
                <Error>0.1</Error>
                <LowerBound>-5000.</LowerBound>
                <UpperBound>5000.</UpperBound>
                <Fixed>true</Fixed>
            </ModelParameter>

            <ModelParameter>
                <Name>M3</Name>
                <Value>4000.</Value>
                <Error>0.1</Error>
                <LowerBound>-5000.</LowerBound>
                <UpperBound>5000.</UpperBound>
                <Fixed>true</Fixed>
            </ModelParameter>

            <ModelParameter>
                <Name>Mu</Name>
                <Value>4000.</Value>
                <Error>0.1</Error>
                <LowerBound>-5000.</LowerBound>
                <UpperBound>5000.</UpperBound>
                <Fixed>true</Fixed>
            </ModelParameter>

            <ModelParameter>
                <Name>MA</Name>
                <Value>4000.</Value>
                <Error>0.1</Error>
                <LowerBound>-5000.</LowerBound>
                <UpperBound>5000.</UpperBound>
                <Fixed>true</Fixed>
            </ModelParameter>

            <ModelParameter>
                <Name>TanBeta</Name>
                <Value>10.</Value>
                <Error>0.1</Error>
                <LowerBound>0.</LowerBound>
                <UpperBound>100.</UpperBound>
                <Fixed>true</Fixed>
            </ModelParameter>

            <ModelParameter>
                <Name>lambda'_232</Name>
                <Value>0.0</Value>
                <Error>0.1</Error>
                <LowerBound>-0.10</LowerBound>
                <UpperBound>+0.11</UpperBound>
                <Fixed>true</Fixed>
            </ModelParameter>

            <ModelParameter>
                <Name>lambda'_233</Name>
                <Value>0.0</Value>
                <Error>0.1</Error>
                <LowerBound>-0.10</LowerBound>
                <UpperBound>+0.11</UpperBound>
                <Fixed>true</Fixed>
            </ModelParameter>

            <ModelParameter>
                <Name>lambda'_313</Name>
                <Value>0.0</Value>
                <Error>0.2</Error>
                <LowerBound>-1.0</LowerBound>
                <UpperBound>+1.1</UpperBound>
                <Fixed>true</Fixed>
            </ModelParameter>

            <ModelParameter>
                <Name>lambda'_323</Name>
                <Value>0.0</Value>
                <Error>0.2</Error>
                <LowerBound>-1.0</LowerBound>
                <UpperBound>+1.1</UpperBound>
                <Fixed>true</Fixed>
            </ModelParameter>
            
            <ModelParameter>
                <Name>lambda'_332</Name>
                <Value>0.0</Value>
                <Error>1.0</Error>
                <LowerBound>-2.0</LowerBound>
                <UpperBound>+2.1</UpperBound>
                <Fixed>true</Fixed>
            </ModelParameter>

            <ModelParameter>
                <Name>lambda'_333</Name>
                <Value>0.0</Value>
                <Error>0.2</Error>
                <LowerBound>-1.0</LowerBound>
                <UpperBound>+1.1</UpperBound>
                <Fixed>true</Fixed>
            </ModelParameter>
            
            <ModelParameter>
                <Name>lambda_322</Name>
                <Value>0.0</Value>
                <Error>1.0</Error>
                <LowerBound>-1.0</LowerBound>
                <UpperBound>+1.1</UpperBound>
                <Fixed>true</Fixed>
            </ModelParameter>

            <ModelParameter>
                <Name>md2_33</Name>
                <Value>16e6</Value>
                <Error>2.5e5</Error>
                <LowerBound>7.5e5</LowerBound>
                <UpperBound>20.1e5</UpperBound>
                <Fixed>true</Fixed>
            </ModelParameter>

            <ModelParameter>
                <Name>mq2_33</Name>
                <Value>16e6</Value>
                <Error>5e5</Error>
                <LowerBound>5e5</LowerBound>
                <UpperBound>2.001e6</UpperBound>
                <Fixed>true</Fixed>
            </ModelParameter>

            <ModelParameter>
                <Name>ml2_33</Name>
                <Value>16e6</Value>
                <Error>5e5</Error>
                <LowerBound>0</LowerBound>
                <UpperBound>2.001e6</UpperBound>
                <Fixed>true</Fixed>
            </ModelParameter>

            <ModelParameter>
                <Name>mf2_high</Name>
                <Value>16e6</Value>
                <Error>5e5</Error>
                <LowerBound>0</LowerBound>
                <UpperBound>2.001e23</UpperBound>
                <Fixed>true</Fixed>
            </ModelParameter>

            <Observables>
            </Observables>
            <CovarianceMatrices>
            </CovarianceMatrices>

            <Calculators>

                <SARAHSPhenoCalculator>
                    <Executable>@SPheno_MSSMTriLnV_EXECUTABLE@</Executable>
                    <InputFile>
                        <Template>@Fittino_INPUT_DIR@/external/SPheno/LesHouches.in.MSSMTriLnV_low</Template>
                        <Name>LesHouches.in.MSSMTriLnV_low</Name>
                        <Quantity>
                            <Name>_M1_</Name>
                            <Value>[M1]</Value>
                        </Quantity>
                        <Quantity>
                            <Name>_M2_</Name>
                            <Value>[M2]</Value>
                        </Quantity>
                        <Quantity>
                            <Name>_M3_</Name>
                            <Value>[M3]</Value>
                        </Quantity>
                        <Quantity>
                            <Name>_MA_</Name>
                            <Value>[MA]</Value>
                        </Quantity>
                        <Quantity>
                            <Name>_Mu_</Name>
                            <Value>[Mu]</Value>
                        </Quantity>
                        <Quantity>
                            <Name>_TanBeta_</Name>
                            <Value>[TanBeta]</Value>
                        </Quantity>
                        <Quantity>
                            <Name>lambda'_232</Name>
                            <Value>[lambda'_232]</Value>
                        </Quantity>
                        <Quantity>
                            <Name>lambda'_233</Name>
                            <Value>[lambda'_233]</Value>
                        </Quantity>
                        <Quantity>
                            <Name>lambda'_313</Name>
                            <Value>[lambda'_313]</Value>
                        </Quantity>
                        <Quantity>
                            <Name>lambda'_323</Name>
                            <Value>[lambda'_323]</Value>
                        </Quantity>
                        <Quantity>
                            <Name>lambda'_332</Name>
                            <Value>[lambda'_332]</Value>
                        </Quantity>
                        <Quantity>
                            <Name>lambda'_333</Name>
                            <Value>[lambda'_333]</Value>
                        </Quantity>
                        <Quantity>
                            <Name>lambda_322</Name>
                            <Value>[lambda_322]</Value>
                        </Quantity>
                        <Quantity>
                            <Name>md2_33</Name>
                            <Value>[md2_33]</Value>
                        </Quantity>
                        <Quantity>
                            <Name>mq2_33</Name>
                            <Value>[mq2_33]</Value>
                        </Quantity>
                        <Quantity>
                            <Name>ml2_33</Name>
                            <Value>[ml2_33]</Value>
                        </Quantity>
                        <Quantity>
                            <Name>mf2_high</Name>
                            <Value>[mf2_high]</Value>
                        </Quantity>
                    </InputFile>
                    <OutputFile>SPheno.spc.MSSMTriLnV</OutputFile>
                </SARAHSPhenoCalculator>

                <RPVCalculator>
                    <Mass_sbottomR>TMath::Sqrt([md2_33])</Mass_sbottomR>
                    <lambdaPrime_313>[lambda'_313]</lambdaPrime_313>
                    <lambdaPrime_323>[lambda'_323]</lambdaPrime_323>
                    <lambdaPrime_333>[lambda'_333]</lambdaPrime_333>
                    <Vev>[SPheno_HMIX_Vev]</Vev>
                </RPVCalculator>

                <FlavioCalculator>
                </FlavioCalculator>
                
                <Chi2Calculator>
                    <Name>R_Kstar0</Name>
                    <Prefix>R_Kstar0</Prefix>
                    <Observable>
                        <Name>LHCb_lowq2</Name>
                        <Prediction>[Flavio_R_Kstar0_LHCb_lowq2]</Prediction>
                        <Measurement>0.66</Measurement>
                        <Uncertainty>
                            <Value>([Prediction]&gt;[Measurement])*0.11+([Prediction]&lt;[Measurement])*0.07</Value>
                        </Uncertainty>
                        <Uncertainty>
                            <Value>0.03</Value>
                        </Uncertainty>
                    </Observable>
                    <Observable>
                        <Name>LHCb_centralq2</Name>
                        <Prediction>[Flavio_R_Kstar0_LHCb_centralq2]</Prediction>
                        <Measurement>0.69</Measurement>
                        <Uncertainty>
                            <Value>([Prediction]&gt;[Measurement])*0.11+([Prediction]&lt;[Measurement])*0.07</Value>
                        </Uncertainty>
                        <Uncertainty>
                            <Value>0.05</Value>
                        </Uncertainty>
                    </Observable>
                </Chi2Calculator>
                <Chi2Calculator>
                    <Name>R_K</Name>
                    <Prefix>R_K</Prefix>
                <Observable>
                    <Name>LHCb</Name>
                    <Prediction>[Flavio_R_K_LHCb]</Prediction>
                    <Measurement>0.745</Measurement>
                    <Uncertainty>
                        <Value>([Prediction]&gt;[Measurement])*0.090+([Prediction]&lt;[Measurement])*0.074</Value>
                    </Uncertainty>
                    <Uncertainty>
                        <Value>0.036</Value>
                    </Uncertainty>
                </Observable>
                </Chi2Calculator>
                
                <FormulaCalculator>
                    <Prefix></Prefix>
                    <Formula>
                        <Name>TotalChi2</Name>
                        <Value>[R_K_Chi2]+[R_Kstar0_Chi2]</Value>
                    </Formula>
                </FormulaCalculator>


            </Calculators>

        </ModelBase>

    </Model>

    <Tool>

        <SimpleSampler>
            <OutputFile>MSSMTriLnV_Raw.root</OutputFile>
        </SimpleSampler>

    </Tool>


</InputFile>

