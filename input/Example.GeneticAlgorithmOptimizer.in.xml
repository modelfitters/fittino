<!DOCTYPE InputFile PUBLIC "" "InputFile.dtd">

<InputFile>

  <VerbosityLevel
   Level = "ALWAYS"
   />

  <ExecutionMode
   Mode = "OPTIMIZATION"
   />

  <Model
   ModelType  = "Rosenbrock"
   Dimensions = "2"
   X1         = "2.56"
   X2         = "-1.54"
   />

  <Optimizer
   AbortCriterium     = "1e-5"
   NumberOfIterations = "500"
   OptimizerType      = "GeneticAlgorithm"
    />

  <GeneticAlgorithmOptimizer
   MutationRate     = "0.5"
   SizeOfPopulation = "100"
   />

</InputFile>
