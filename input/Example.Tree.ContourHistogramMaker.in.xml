<InputFile>

  <VerbosityLevel>ALWAYS</VerbosityLevel>

  <Model>

    <ModelBase>

      <Name>Tree</Name>
      <Tag>Plot</Tag>

      <ModelParameter>
        <Name>Entry</Name>
        <!-- Set Value to the best fit entry, determined e.g. by running SimpleOptimizer. -->
        <Value>681180</Value>
        <Error>1</Error>
        <LowerBound>0</LowerBound>
        <UpperBound>4316375</UpperBound>
      </ModelParameter>

      <Calculators>
        <TreeCalculator>
          <InputFileName>/nfs/dust/atlas/user/sarrazin/HDim6/Fit/2/Skimmed4.root</InputFileName>
        </TreeCalculator>
      </Calculators>

      <Observables></Observables>

      <CovarianceMatrices></CovarianceMatrices>

      <Chi2Contribution>[Chi2]</Chi2Contribution>

    </ModelBase>

  </Model>

  <Tool>

    <ContourHistogramMaker>

      <Histogram>
        <Name>f_WW_Value</Name>
        <PlotName>f_WW</PlotName>
        <NumberOfBins>200</NumberOfBins>
        <LowerBound>-50.</LowerBound>
        <UpperBound>+50.</UpperBound>
      </Histogram>

      <Histogram>
        <Name>f_BB_Value</Name>
        <PlotName>f_BB</PlotName>
        <NumberOfBins>200</NumberOfBins>
        <LowerBound>-50.</LowerBound>
        <UpperBound>50.</UpperBound>
      </Histogram>

      <Histogram>
        <Name>f_W</Name>
        <PlotName>f_W</PlotName>
        <NumberOfBins>50</NumberOfBins>
        <LowerBound>-55.</LowerBound>
        <UpperBound>+55.</UpperBound>
      </Histogram>

      <Histogram>
        <Name>f_B</Name>
        <PlotName>f_B</PlotName>
        <NumberOfBins>50</NumberOfBins>
        <LowerBound>-110.</LowerBound>
        <UpperBound>+110.</UpperBound>
      </Histogram>

      <Plotter>
        <ContourPlotter>
          <PageFormat>Square</PageFormat>
          <Style>Classic</Style>
        </ContourPlotter>
      </Plotter>

    </ContourHistogramMaker>

  </Tool>

</InputFile>
