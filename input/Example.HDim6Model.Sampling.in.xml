<!DOCTYPE InputFile PUBLIC "" "InputFile.dtd">

<InputFile>

  <VerbosityLevel
   Level = "INFO"
   />

  <ExecutionMode
   Mode = "SAMPLING"
   />

  <Model
   ModelType = "HDim6"
   />

  <Parameter
   Type             = "Physics"
   Name             = "f_B"
   Value            = "0"
   Error            = "1e-4"
   LowerBound       = "-1e-1"
   UpperBound       = "+1e-1"
   Fixed            = "1"
  />

  <Parameter
   Type             = "Physics"
   Name             = "f_BB"
   Value            = "0"
   Error            = "1e-4"
   LowerBound       = "-1e-1"
   UpperBound       = "+1e-1"
   Fixed            = "1"
  />

  <Parameter
   Type             = "Physics"
   Name             = "f_W"
   Value            = "0"
   Error            = "1e-4"
   LowerBound       = "-1e-1"
   UpperBound       = "+1e-1"
   Fixed            = "1"
  />

  <Parameter
   Type             = "Physics"
   Name             = "f_WW"
   Value            = "0"
   Error            = "1e-3"
   LowerBound       = "-5e-3"
   UpperBound       = "+5e-3"
   Fixed            = "0"
  />

  <Parameter
   Type             = "Physics"
   Name             = "f_GG"
   Value            = "0"
   Error            = "1e-4"
   LowerBound       = "-1e-1"
   UpperBound       = "+1e-1"
   Fixed            = "1"
  />

  <Parameter
   Type             = "Physics"
   Name             = "f_Phi_2"
   Value            = "0"
   Error            = "1e-4"
   LowerBound       = "-1e-1"
   UpperBound       = "+1e-1"
   Fixed            = "1"
  />

  <Parameter
   Type             = "Physics"
   Name             = "f_t"
   Value            = "0"
   Error            = "1e-4"
   LowerBound       = "-1e-1"
   UpperBound       = "+1e-1"
   Fixed            = "1"
  />

 
  <Parameter
   Type             = "Physics"
   Name             = "f_b"
   Value            = "0"
   Error            = "1e-4"
   LowerBound       = "-1e-1"
   UpperBound       = "+1e-1"
   Fixed            = "1"
  />

  <Parameter
   Type             = "Physics"
   Name             = "f_tau"
   Value            = "0"
   Error            = "1e-4"
   LowerBound       = "-1e-1"
   UpperBound       = "+1e-1"
   Fixed            = "1"
  />

 <Parameter
   Type             = "Physics"
   Name             = "mass_h"
   Value            = "125.5"
   Error            = "0"
   LowerBound       = "125"
   UpperBound       = "126"
   Fixed            = "1"
 />

 <HiggsSignalsHadXSCalculator>
    <ExpData>  latestresults </ExpData>
<!--     <ExpData>  mail_14_07_2013_HS_new_observable_set </ExpData>  -->
 </HiggsSignalsHadXSCalculator>  

  <Sampler
   SamplerType = "MarkovChain"
   NumberOfIterations = "1"
   />

</InputFile>
