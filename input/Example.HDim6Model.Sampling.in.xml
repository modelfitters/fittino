  <VerbosityLevel
   Level = "INFO"
   />

  <ExecutionMode
   Mode = "SAMPLING"
   />

  <Model
   ModelType = "HDim6"
   />

  <Sampler
   NumberOfIterations = "1"
   SamplerType = "MarkovChain"

   />

