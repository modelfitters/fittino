<?xml version="1.0" encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="style/InputFile.xsl"?>

<InputFile>

  <VerbosityLevel>INFO</VerbosityLevel>

  <Model>
    <PhysicsModel>

      <Name>HDim6Model</Name>

      <ModelParameter>
        <Name>f_B</Name>
        <Value>5</Value>
        <Error>1</Error>
        <LowerBound>-1000</LowerBound>
        <UpperBound>+1000</UpperBound>
        <Fixed>true</Fixed>       
      </ModelParameter>

      <ModelParameter>
        <Name>f_VV_plus</Name>
        <Value>5</Value>
        <Error>1</Error>
        <LowerBound>-1000</LowerBound>
        <UpperBound>+1000</UpperBound>
        <Fixed>false</Fixed>       
      </ModelParameter>

      <ModelParameter>
        <Name>f_VV_minus</Name>
        <Value>5</Value>
        <Error>1</Error>
        <LowerBound>-1000</LowerBound>
        <UpperBound>+1000</UpperBound>
        <Fixed>false</Fixed>       
      </ModelParameter>

      <ModelParameter>
        <Name>f_W</Name>
        <Value>5</Value>
        <Error>1</Error>
        <LowerBound>-1000</LowerBound>
        <UpperBound>+1000</UpperBound>
        <Fixed>true</Fixed>       
      </ModelParameter>

      <ModelParameter>
        <Name>f_GG</Name>
        <Value>0</Value>
        <Error>1</Error>
        <LowerBound>-1000</LowerBound>
        <UpperBound>+1000</UpperBound>
        <Fixed>false</Fixed>       
      </ModelParameter>

      <ModelParameter>
        <Name>f_Phi_2</Name>
        <Value>0</Value>
        <Error>1</Error>
        <LowerBound>-1000</LowerBound>
        <UpperBound>+1000</UpperBound>
        <Fixed>false</Fixed>       
      </ModelParameter>

      <ModelParameter>
        <Name>f_t</Name>
        <Value>0</Value>
        <Error>1</Error>
        <LowerBound>-1000</LowerBound>
        <UpperBound>+1000</UpperBound>
        <Fixed>false</Fixed>       
      </ModelParameter>

      <ModelParameter>
        <Name>f_b</Name>
        <Value>0</Value>
        <Error>1</Error>
        <LowerBound>-1000</LowerBound>
        <UpperBound>+1000</UpperBound>
        <Fixed>false</Fixed>       
      </ModelParameter>

      <ModelParameter>
        <Name>f_tau</Name>
        <Value>0</Value>
        <Error>1</Error>
        <LowerBound>-1000</LowerBound>
        <UpperBound>+1000</UpperBound>
        <Fixed>false</Fixed>       
      </ModelParameter>

      <ModelParameter>
        <Name>mass_h</Name>
        <Value>125.5</Value>
        <Error>0</Error>
        <LowerBound>125</LowerBound>
        <UpperBound>126</UpperBound>
        <Fixed>true</Fixed>       
      </ModelParameter>

      <Calculators>

        <HDim6Calculator>
          <Name>HDim6Calculator</Name>
          <calculate_Gamma_hWW>true</calculate_Gamma_hWW>
          <calculate_Gamma_hZZ>true</calculate_Gamma_hZZ>
          <calculate_xs_qqh_2flavor>true</calculate_xs_qqh_2flavor>
          <calculate_xs_qqh_5flavor>true</calculate_xs_qqh_5flavor>
          <calculate_xs_Wh>true</calculate_xs_Wh>
          <calculate_xs_Zh>true</calculate_xs_Zh>
          <UnitarityCoefficientR>1000.0</UnitarityCoefficientR>
          <UnitarityCoefficientN>1.0</UnitarityCoefficientN>
          <OverrideUnitarity>true</OverrideUnitarity>
        </HDim6Calculator>

        <HiggsSignalsCalculator>
        <Name>HiggsSignalsCalculator</Name>
        <ExpData>LHC_latestresults-1.2.0-CMSgaga125p7</ExpData>
        </HiggsSignalsCalculator>

      </Calculators>

      <Observables></Observables>

      <CovarianceMatrices></CovarianceMatrices>

      <Chi2Contribution>HS_chi2_mu</Chi2Contribution>

    </PhysicsModel>
  </Model>

  <Tool>
  <MarkovChainSampler>
    <NumberOfIterations>1</NumberOfIterations>
    <RandomSeed>1</RandomSeed>
  </MarkovChainSampler>
  </Tool>

</InputFile>

