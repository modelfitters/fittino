<?xml version="1.0" encoding='UTF-8'?>

<InputFile>

  <VerbosityLevel>ALWAYS</VerbosityLevel>

  <Model>
    <PhysicsModel>

      <Name>HDim6Model</Name>

      <ModelParameter>
        <name>f_B</name>
        <value>0</value>
        <error>1e-6</error>
        <lowerBound>-1e-2</lowerBound>
        <upperBound>+1e-2</upperBound>
        <fixed>true</fixed>       
      </ModelParameter>

      <ModelParameter>
        <name>f_BB_p_f_WW</name>
        <value>0</value>
        <error>1e-6</error>
        <lowerBound>-1e-2</lowerBound>
        <upperBound>+1e-2</upperBound>
        <fixed>false</fixed>       
      </ModelParameter>

      <ModelParameter>
        <name>f_BB_m_f_WW</name>
        <value>0</value>
        <error>1e-6</error>
        <lowerBound>-1e-2</lowerBound>
        <upperBound>+1e-2</upperBound>
        <fixed>false</fixed>       
      </ModelParameter>

      <ModelParameter>
        <name>f_W</name>
        <value>0</value>
        <error>1e-6</error>
        <lowerBound>-1e-2</lowerBound>
        <upperBound>+1e-2</upperBound>
        <fixed>true</fixed>       
      </ModelParameter>

      <ModelParameter>
        <name>f_GG</name>
        <value>0</value>
        <error>1e-6</error>
        <lowerBound>-1e-2</lowerBound>
        <upperBound>+1e-2</upperBound>
        <fixed>false</fixed>       
      </ModelParameter>

      <ModelParameter>
        <name>f_Phi_2</name>
        <value>0</value>
        <error>1e-6</error>
        <lowerBound>-1e-2</lowerBound>
        <upperBound>+1e-2</upperBound>
        <fixed>false</fixed>       
      </ModelParameter>

      <ModelParameter>
        <name>f_t</name>
        <value>0</value>
        <error>1e-6</error>
        <lowerBound>-1e-2</lowerBound>
        <upperBound>+1e-2</upperBound>
        <fixed>false</fixed>       
      </ModelParameter>

      <ModelParameter>
        <name>f_b</name>
        <value>0</value>
        <error>1e-6</error>
        <lowerBound>-1e-2</lowerBound>
        <upperBound>+1e-2</upperBound>
        <fixed>false</fixed>       
      </ModelParameter>

      <ModelParameter>
        <name>f_tau</name>
        <value>0</value>
        <error>1e-6</error>
        <lowerBound>-1e-2</lowerBound>
        <upperBound>+1e-2</upperBound>
        <fixed>false</fixed>       
      </ModelParameter>

      <ModelParameter>
        <name>mass_h</name>
        <value>125.5</value>
        <error>0</error>
        <lowerBound>125</lowerBound>
        <upperBound>126</upperBound>
        <fixed>true</fixed>       
      </ModelParameter>

      <Calculators>

        <HDim6Calculator>
        <name>HDim6Calculator</name>
        </HDim6Calculator>

        <RegressionCalculator>
          <name>RegressionCalculator</name>
          <Variable>
            <Name>f_BB_m_f_WW</Name>
            <NameInWeightFiles>f_BB_m_f_WW</NameInWeightFiles>
          </Variable>
          <Variable>
            <Name>f_BB_p_f_WW</Name>
            <NameInWeightFiles>f_BB_p_f_WW</NameInWeightFiles>
          </Variable>
          <Variable>
            <Name>f_Phi_2</Name>
            <NameInWeightFiles>f_Phi_2</NameInWeightFiles>
          </Variable>
          <MVA>
            <WeightFile>TMVARegression_hWW.weights.xml</WeightFile>
            <Target>normSM_Gamma_hWW</Target>
          </MVA>
        </RegressionCalculator>

        <RegressionCalculator>
          <name>RegressionCalculator2</name>
          <Variable>
            <Name>f_BB_m_f_WW</Name>
            <NameInWeightFiles>f_BB_m_f_WW</NameInWeightFiles>
          </Variable>
          <Variable>
            <Name>f_BB_p_f_WW</Name>
            <NameInWeightFiles>f_BB_p_f_WW</NameInWeightFiles>
          </Variable>
          <Variable>
            <Name>f_Phi_2</Name>
            <NameInWeightFiles>f_Phi_2</NameInWeightFiles>
          </Variable>
          <MVA>
            <WeightFile>TMVARegression_hZZ.weights.xml</WeightFile>
            <Target>normSM_Gamma_hZZ</Target>
          </MVA>
        </RegressionCalculator>

        <RegressionCalculator>
          <name>RegressionCalculator3</name>
          <Variable>
            <Name>f_BB_m_f_WW</Name>
            <NameInWeightFiles>f_BB_m_f_WW</NameInWeightFiles>
          </Variable>
          <Variable>
            <Name>f_BB_p_f_WW</Name>
            <NameInWeightFiles>f_BB_p_f_WW</NameInWeightFiles>
          </Variable>
          <Variable>
            <Name>f_Phi_2</Name>
            <NameInWeightFiles>f_Phi_2</NameInWeightFiles>
          </Variable>
          <MVA>
            <WeightFile>TMVARegression_qqh.weights.xml</WeightFile>
            <Target>normSM_xs_qqh_2flavor</Target>
          </MVA>
        </RegressionCalculator>


        <RegressionCalculator>
          <name>RegressionCalculator4</name>
          <Variable>
            <Name>f_BB_m_f_WW</Name>
            <NameInWeightFiles>f_BB_m_f_WW</NameInWeightFiles>
          </Variable>
          <Variable>
            <Name>f_BB_p_f_WW</Name>
            <NameInWeightFiles>f_BB_p_f_WW</NameInWeightFiles>
          </Variable>
          <Variable>
            <Name>f_Phi_2</Name>
            <NameInWeightFiles>f_Phi_2</NameInWeightFiles>
          </Variable>
          <MVA>
            <WeightFile>TMVARegression_Wh.weights.xml</WeightFile>
            <Target>normSM_xs_Wh</Target>
          </MVA>
        </RegressionCalculator>


        <RegressionCalculator>
          <name>RegressionCalculator5</name>
          <Variable>
            <Name>f_BB_m_f_WW</Name>
            <NameInWeightFiles>f_BB_m_f_WW</NameInWeightFiles>
          </Variable>
          <Variable>
            <Name>f_BB_p_f_WW</Name>
            <NameInWeightFiles>f_BB_p_f_WW</NameInWeightFiles>
          </Variable>
          <Variable>
            <Name>f_Phi_2</Name>
            <NameInWeightFiles>f_Phi_2</NameInWeightFiles>
          </Variable>
          <MVA>
            <WeightFile>TMVARegression_Zh.weights.xml</WeightFile>
            <Target>normSM_xs_Zh</Target>
          </MVA>
        </RegressionCalculator>

        <HiggsSignalsCalculator>
        <name>HiggsSignalsCalculator</name>
        <ExpData>LHC_latestResults_25_10_2013</ExpData>
        </HiggsSignalsCalculator>

      </Calculators>

      <Chi2Contribution>HS_chi2_mu</Chi2Contribution>

    </PhysicsModel>
  </Model>

  <Tool>
  <MarkovChainSampler>
    <numberOfIterations>100000</numberOfIterations>
    <randomSeed>0</randomSeed>
  </MarkovChainSampler>
  </Tool>

</InputFile>

