<!DOCTYPE InputFile PUBLIC "" "InputFile.dtd">

<InputFile>

  <VerbosityLevel
   Level = "ALWAYS"
   />

  <ExecutionMode
   Mode = "SAMPLING"
   />

  <Model
   ModelType = "CMSSM"
   A0        = "-300."
   M0        = "100."
   M12       = "250."
   TanBeta   = "10."
   />
  
  <Observable
   Name          = "O_massNeutralino1"
   MeasuredValue = "150"
   Error1        = "5"
   />
  
  <Observable
   Name          = "O_massNeutralino2"
   MeasuredValue = "230"
   Error1        = "5"
   />
  <Sampler
   NumberOfIterations = "500"
   PerformToyRun      = "0"
   InputFileName      = "Fittino.old.root"
   InputTreeName      = "Tree"
   SamplerType        = "Tree"
   />
</InputFile>
